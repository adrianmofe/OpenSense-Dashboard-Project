<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui" 
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html" xmlns:m="urn:import:gwt.material.design.client.ui">
	
	<ui:with type="com.opensense.dashboard.client.utils.Languages" field="lang" />
		
	<ui:with type="com.opensense.dashboard.client.gui.GUIImageBundle" field="gui" />
	
<ui:style>
.bottom-con{
	position: absolute;
	width: 90px;
	bottom: 0px;
}
</ui:style>

<bh:Div addStyleNames="nav-container vertical user-select-none">
	<b:ButtonGroup ui:field="buttonGroup">
		<b:Button ui:field="homeButton" addStyleNames="nav-button" />
		<b:Button ui:field="searchButton" addStyleNames="nav-button" />
		<b:Button ui:field="mapButton" addStyleNames="nav-button" />
		<b:Button ui:field="visualisationsButton" addStyleNames="nav-button" />
		<b:Button ui:field="listButton" addStyleNames="nav-button" />
	</b:ButtonGroup>
	<bh:Div ui:field="lastButtonCon" addStyleNames="{style.bottom-con}">
		<b:Button ui:field="userButton" addStyleNames="nav-button" />
	</bh:Div>
</bh:Div>

</ui:UiBinder>